	android:layout_width="match_parent"
	android:layout_height="match_parent">
	
	<LinearLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		tools:context="bk.myapp.NotificationReceiver">
		
		<android.support.design.widget.AppBarLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			
			<android.support.v7.widget.Toolbar
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@color/colorPrimaryDark">
			
			</android.support.v7.widget.Toolbar>
		</android.support.design.widget.AppBarLayout>
		
		<ru.rambler.libs.swipe_layout.SwipeLayout
			xmlns:android="http://schemas.android.com/apk/res/android"
			xmlns:app="http://schemas.android.com/apk/res-auto"
			android:id="@+id/swipe_layout"
			android:layout_width="match_parent"
			android:layout_height="100dp"
			android:layout_marginBottom="5dp"
			android:layout_marginLeft="20dp"
			android:layout_marginRight="20dp"
			android:layout_marginTop="10dp">
			
			<!--CENTER. app:gravity="center" - is default value -->
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="horizontal">
				
				<TextView
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="0.3"
					android:background="@drawable/tv_stroke"
					android:gravity="center"
					android:padding="10dp"
					android:text="@string/_2_1_km"
					android:textColor="#000"
					android:textSize="15dp"
					android:textStyle="bold"/>
				
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:background="@drawable/tv_grad"
					android:orientation="vertical">
					
					<TextView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="0.75"
						android:gravity="center"
						android:text="VIT University Vellore"
						android:textColor="#fff"
						android:textSize="20dp"
						android:textStyle="bold"/>
					
					<TextView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="0.35"
						android:gravity="top"
						android:paddingLeft="25dp"
						android:text="Accident"
						android:textColor="#fff"
						android:textStyle="bold"
						/>
				</LinearLayout>
			</LinearLayout>
			
			<!--RIGHT-->
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@drawable/tv_stroke"
				android:orientation="horizontal"
				app:bring_to_clamp="150dp"
				app:clamp="self"
				android:onClick="goToMapActivity"
				app:gravity="left"
				app:sticky="100dp">
				
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="0.5"
					android:orientation="vertical">
					
					<ImageView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="0.75"
						android:src="@mipmap/car"/>
					
					<TextView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="0.25"
						android:gravity="center"
						android:text="Vehicle"/>
				
				</LinearLayout>
				
				<View
					android:layout_width="3dp"
					android:layout_height="match_parent"
					android:background="#693fbe"/>
				
				<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="0.5"
					android:orientation="vertical">
					
					<ImageView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="0.75"
						android:padding="5dp"
						android:src="@drawable/bgplus"/>
					
					<TextView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="0.25"
						android:gravity="center"
						android:text="First Aid"/>
				
				</LinearLayout>
			</LinearLayout>
			
			<!--LEFT-->
			<FrameLayout
				android:layout_width="200dp"
				android:layout_height="match_parent"
				app:bring_to_clamp="100dp"
				app:clamp="self"
				app:gravity="right"
				app:sticky="none">
			
			</FrameLayout>
		</ru.rambler.libs.swipe_layout.SwipeLayout>
		
